@using ViewModels
@model TaskTypeDetailViewModel
@{
    Layout = null;
}

<div class="row">
    <div class="col-lg-12">
        <div class="row">
            <div class="col-lg-12">
                <div class="mb-3 form-group d-flex">
                    <label asp-for="Code" class="form-label detail-label fw-600"></label>
                    <div>@(string.IsNullOrEmpty(Model.Code) ? "-" : Model.Code)</div>
                </div>
            </div>
        </div>
    </div>
</div>
