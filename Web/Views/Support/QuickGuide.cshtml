@{
    ViewBag.Title = "Quick Guide";
}

<div class="userguid-support">
    <div class="col-md-12">
        <embed src="/PDF/Quick_Guide.pdf" id="pageemb" file_name.pdf" width="100%" height="700px" />
    </div>

</div>

<script>$(function () {
        $("#main-section-container").removeClass("py-3");
        $("#main-section-container").find("div.container-fluid").first().removeClass("container-fluid");
        var height = $(window).height();
        var footer = $(".footer").height();
        var header = $(".header").height();
        var title = $(".page-title").height();
        $("#pageemb").css("height", (height - footer - header - title - 80) + "px");
    })</script>