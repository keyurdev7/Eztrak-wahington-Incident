@model ViewModels.BaseSelect2VM

@{
    var elementId = ViewData["Id"] != null && !string.IsNullOrEmpty(ViewData["Id"].ToString()) ? ViewData["Id"].ToString() : "condition-id";
}
<partial name="~/Views/Shared/Select2/Condition/_ConditionHtml.cshtml" model="@Model" />
<partial name="~/Views/Shared/Select2/Condition/_ConditionJs.cshtml" model="@Model" />
<script src="~/js/custom/select2/select2-methods.js"></script>
<script>
    $(function () {
        var id = "#" + "@elementId";
        SetConditionSelect2(id);
    });
</script>
