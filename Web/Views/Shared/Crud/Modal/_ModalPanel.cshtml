@{
    Layout = null;
    var id = ViewData["modalId"];
    var title = ViewData["modalTitle"];
    var minWidth = ViewData["modalMinWidth"] ?? "65%";
    var zIndex = ViewData["modalZIndex"] ?? "9991";

}
<div class="modal fade" data-backdrop="static" id="@id" style="z-index:@zIndex;">
    <div class="modal-dialog modal-md" style="min-width:@minWidth;">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="fs-5 modal-title">@title</h4>
                <button class="close" data-bs-dismiss="modal"><span>&times;</span></button>
            </div>
            <div class="modal-body">
            </div>
        </div>
    </div>
</div>