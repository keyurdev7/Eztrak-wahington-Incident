@using ViewModels.AssignedPermissions;
@model AssignedPermissionDetailViewModel
@{
    Layout = null;
}

<div class="row">
    <div class="col-lg-12">
        <div class="row">
            <div class="col-lg-12">
                <div class="mb-3 form-group">
                    <label asp-for="Permission.Name" class="form-label fw-600"></label>
                    <div>@(string.IsNullOrEmpty(Model.Permission.Name) ? "-" : Model.Permission.Name)</div>
                </div>
            </div>
        </div>
    </div>
</div>