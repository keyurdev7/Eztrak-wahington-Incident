@using ViewModels
@using ViewModels.Shared.Notes
@model List<INotesViewModel>

@{
    Layout = null;
    var WorkOrderId = ViewBag.WorkOrderId;
}

@await Html.PartialAsync("~/Views/Shared/Notes/_NotesPartialView.cshtml", Model, new ViewDataDictionary(ViewData) { { "ParentId", WorkOrderId }, { "ParentName", "WorkOrderId" } })


<script src="~/js/views/notes/notes.js"></script>
<script>
    notesUrl = "/WorkOrder/SaveNotes";
    closeNotesModalOnSubmit = false;
    function getNotesList() {
        $.get("/WorkOrder/GetNotesListPartialView/" + @WorkOrderId, function (response) {
            $("#notes-list-container").html(response);
        });
    }
</script>