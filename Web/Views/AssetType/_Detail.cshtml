@using ViewModels
@model AssetTypeDetailViewModel
@{
    Layout = null;
}

<div class="row">
    <div class="col-lg-12">
        <div class="row">
            <div class="col-lg-12">
                <div class="mb-3 form-group">
                    <label asp-for="Name" class="form-label fw-600"></label>
                    <div>@(string.IsNullOrEmpty(Model.Name) ? "-" : Model.Name)</div>
                </div>
                <div class="mb-3 form-group">
                    <label asp-for="Color" class="form-label fw-600"></label>
                    <div>@(string.IsNullOrEmpty(Model.Color) ? "-" : Model.Color)</div>
                </div>
            </div>
        </div>
    </div>
</div>
